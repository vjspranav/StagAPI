{
    "name": "delete-test-01",
    "description": "delete-test-01-description",
    "version": "0.0.1",
    "private": true,
    "dependencies": {
        "mongodb": "^6.3.0"
    },
    "devDependencies": {
        "aws-sdk-client-mock": "^2.0.0",
        "jest": "^29.2.1"
    },
    "scripts": {
        "test": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js",
        "deploy-13": "sam build && export $(cat .env | xargs) && sam deploy --config-env 13 --parameter-overrides MONGOURI=$MONGOURI VERSION=13 STAGE=13",
        "deploy": "sam build && export $(cat .env | xargs) && sam deploy --config-env default --parameter-overrides MONGOURI=$MONGOURI STAGE=Prod" 
    },
    "jest": {
        "testMatch": [
            "**/__tests__/**/*.[jt]s?(x)",
            "**/?(*.)+(spec|test).[jt]s?(x)",
            "**/__tests__/**/*.mjs?(x)",
            "**/?(*.)+(spec|test).mjs?(x)"
        ],
        "moduleFileExtensions": [
            "mjs",
            "js"
        ]
    }
}
